<!DOCTYPE html>
<html>
<head>
  <title>Protected Page</title>
</head>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css
">
<body>
  <h1>Hello <%= displayName %></h1>
  <p>Imagen <%= picture %></p>
  <p>Correo <%= email %></p>
  <p>Your token is: <%= token %></p>
  <button >PRECIONAR PARA REALIZAR UN POST</button>
  <br><br>
  <button><a href="/auth/logout">Sing out</a></button>


  
  <script  >


    console.log(window.location.hostname.includes('localhost'))
// function enviarPost() {

  
  var url = 'http://localhost:5000/api/auth/'
                
  const token = '<%= token %>';
  const nombre = '<%= displayName %>'
  const img='<%= picture %>'
  const correo = '<%= email %>'
  
  // Realizar el fetch POST con el token
  fetch(url + 'google',  {
    method: 'POST',
    headers: {
      'Content-Type': 'application/json'
    },
    body: JSON.stringify({ token , correo , img , nombre})
  })
    .then(response => response.json())
    .then(({token}) => {
      console.log(token)
      localStorage.setItem("token" ,token);
      windows.location = 'chat.html';
    })
    .catch(error => {
      // Manejar errores
      console.error(error);
    });



  </script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>

</body>
</html>